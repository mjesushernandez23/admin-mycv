{
  "kind": "singleType",
  "collectionName": "resumes",
  "info": {
    "name": "resume",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "position": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "skills": {
      "type": "richtext"
    },
    "workExperience": {
      "type": "richtext"
    },
    "study": {
      "type": "richtext"
    },
    "objective": {
      "type": "richtext"
    },
    "additional": {
      "type": "text"
    },
    "contact": {
      "type": "component",
      "repeatable": true,
      "component": "contact-type.contact-list"
    }
  }
